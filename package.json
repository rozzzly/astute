{
    "name": "astute",
    "version": "1.0.0",
    "description": "ast-based syntax highlighting",
    "main": "dist/index.js",
    "repository": "https://github.com/rozzzly/astute.git",
    "author": "rozzzly",
    "license": "MIT",
    "private": true,
    "scripts": {
        "test": "jest",
        "coverage": "jest --coverage",
        "dev": "jest --watch",
        "build:types": "tsc -p ./tsconfig.dist.json --emitDeclarationOnly",
        "build:esm": "TARGET=esm babel src --out-dir bin/esm --extensions \".ts,.tsx\"",
        "build:cjs": "TARGET=cjs babel src --out-dir bin/cjs --extensions \".ts,.tsx\"",
        "dist": "node ./build-and-package.js"
    },
    "devDependencies": {
        "@babel/cli": "^7.12.8",
        "@jest/globals": "^26.6.2",
        "@types/node": "^14.14.10",
        "@typescript-eslint/eslint-plugin": "^4.14.0",
        "@typescript-eslint/parser": "^4.14.0",
        "eslint": "^7.18.0",
        "jest": "^26.6.3",
        "jest-circus": "^26.6.3",
        "jest-cli": "^26.6.3"
    },
    "dependencies": {
        "@babel/core": "^7.12.9",
        "@babel/parser": "^7.12.7",
        "@babel/plugin-proposal-class-properties": "^7.12.1",
        "@babel/plugin-proposal-object-rest-spread": "^7.12.1",
        "@babel/plugin-transform-runtime": "^7.12.1",
        "@babel/preset-env": "^7.12.7",
        "@babel/preset-typescript": "^7.12.7",
        "@babel/traverse": "^7.12.9",
        "@babel/types": "^7.12.7",
        "typescript": "^4.1.2"
    }
}
